$SPARK_HOME/bin/spark-submit \
  --class $1 \
  --master local[2] \
  --conf "spark.executor.extraJavaOptions=-Dorg.dcs.spark.receiver=$2 -Dorg.dcs.spark.sender=$3" \
  --conf "spark.driver.extraJavaOptions=-Dorg.dcs.spark.receiver=$2 -Dorg.dcs.spark.sender=$3" \
  ./org.dcs.spark/target/org.dcs.spark-assembly-0.3.0-SNAPSHOT.jar
